<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.APPHYDROJETTING" parent="Theme.Material3.DayNight.NoActionBar"></style>

    <style name="Theme.APPHYDROJETTING" parent="Base.Theme.APPHYDROJETTING" />

    <style name="Theme.APPHYDROJETTING.Fullscreen" parent="Theme.APPHYDROJETTING">
        <item name="android:actionBarStyle">
            @style/Widget.Theme.APPHYDROJETTING.ActionBar.Fullscreen
        </item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@null</item>
    </style>

    <style name="ThemeOverlay.APPHYDROJETTING.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">@color/light_blue_600</item>
        <item name="fullscreenTextColor">@color/light_blue_A200</item>
    </style>

    <style name="Theme.APPHYDROJETTING.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.APPHYDROJETTING.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.APPHYDROJETTING.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>